# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# defaults
[*]
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8
indent_style = space
indent_size = 4

# Clojure dialect settings
[*.{clj,cljc,cljs}]
indent_style = space
indent_size = 2
